<div class="game-list-container">
<div class="admin-btn-container">
<h1>Games</h1>
<a href="/admin/games/new" class="add-game-btn"> Add game </a>
<a href="/admin/score/new" class="add-game-btn"> Add score </a>
</div>
<table class="table table-striped">
  <thead class="game-list">
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Genre</th>
      <th>Release date</th>
    </tr>
  </thead>
  <tbody>
    <% games.forEach(game=> { %>
    <tr>
      <td><%= game.id %></td>
      <td><%= game.title %></td>
      <td><%= game.genre %></td>
      <td><%= game.release_date %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
</div>